{
  "compilerOptions": {
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "rootDir": "./frontend/",
    "outDir": "./frontend/dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": [
    "./frontend/"
  ]
}